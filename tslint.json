{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "trailing-comma": [ false ],
      "semicolon": [true, "never"],
      "quotemark": [true, "single"],
      "whitespace": true,
      "space-before-function-paren": true,
      "space-within-parens": true,
      "import-spacing": [true]
  },
  "rulesDirectory": []
}