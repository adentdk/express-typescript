{
  "name": "express-typescript",
  "version": "0.0.1",
  "description": "express with typescript",
  "main": "index.js",
  "scripts": {
    "copySHAFile": "ts-node src/utils/copySHAFile",
    "clean:tsc": "rimraf ./dist-ts",
    "clean": "run-p -l clean:*",
    "lint:tsc": "tslint -c tslint.json -p tsconfig.json --fix",
    "prebuild:tsc": "npm-run-all lint:tsc copySHAFile",
    "build:tsc": "tsc --project ./",
    "preprod:tsc": "npm run build:tsc",
    "prod:tsc": "node ./dist-ts/index.js",
    "dev": "nodemon",
    "start": "ts-node ./src/index.ts"
  },
  "repository": "git@github.com:adentdk/express-typescript.git",
  "author": "Aden Trisna Daud Kurnia <adentrisnadaudkurnia@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "http-status-codes": "^2.1.3",
    "prom-client": "^12.0.0",
    "shelljs": "^0.8.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.10.3",
    "@types/shelljs": "^0.8.8",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  }
}
